    width:auto;
    height:130px;
    margin:0px;
    padding:5px;
}

.lodging-photo
{
    position: relative;
    vertical-align: middle;
    float: left;
    width: 22%;
    max-width: 160px;
    max-height: 120px;
}

.lodging-photo .photo
{
    width: 100%;
}

.lodging-description

.jScrollPaneContainer
{
    position: relative; 
    overflow: hidden; 
    z-index: 1; 
}

.jScrollPaneTrack
{
    position: absolute; 
    cursor: pointer; 
    right: 0; 
    top: 0; 
    height: 100%;

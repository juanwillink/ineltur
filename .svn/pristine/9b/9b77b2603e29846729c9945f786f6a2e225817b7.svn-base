/* remember to define focus styles! */
:focus {
        outline: 0;
}
ol, ul {
        list-style: none;
}
caption, th, td {
        text-align: left;
        font-weight: normal;
}
blockquote:before, blockquote:after,
q:before, q:after {
        content: "";
}
blockquote, q {
        quotes: "" "";
}


